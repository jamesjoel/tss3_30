<html>
<head>
    <title>Home Page</title>
</head>
<body>
<h1>List Page</h1>
<table border="1" cellspacing="0" cellpadding="10" align="center">
    <tr>
        <th>S.No.</th>
        <th>Name</th>
        <th>Age</th>
        <th>City</th>
    </tr>
    <% info.forEach(function(x, n){ %>
        <tr>
            <td><%= n+1 %></td>
            <td><%= x.name %></td>
            <td><%= x.age %></td>
            <td><%= x.city %></td>
        </tr>
    <% }) %>
</table>

<h1><%= x.demo %></h1>

</body>
</html>